{
  "name": "react-smartmultiparts",
  "homepage": "http://chrismojekwu.github.io/react-smartmultiparts/",
  "version": "1.5.11",
  "description": "Form generating file upload components for React",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "develop": "webpack serve",
    "test": "jest --watch",
    "build-lib": "rollup -c",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "webpack --config webpack.prod.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "author": "Chris Mojekwu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-loader": "^8.2.2",
    "enzyme": "^3.11.0",
    "gh-pages": "^3.2.0",
    "react": "18.0",
    "react-dom": "18.0",
    "rollup": "^2.46.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^3.2.1",
    "webpack-dev-server": "^4.8.1"
  },
  "peerDependencies": {
    "react": "18.0",
    "react-dom": "18.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.14.4",
    "@cfaester/enzyme-adapter-react-18": "^0.7.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.3.4",
    "react-router-dom": "^5.3.0",
    "react-test-renderer": "18.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  }
}
